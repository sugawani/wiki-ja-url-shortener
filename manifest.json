{
    "author": "sugawani",
    "homepage_url": "https://twitter.com/nek0roll",
    "manifest_version": 2,
    "name": "wiki-url-shortener",
    "description": "wikipedia日本語URLを省略するやつ",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["https://*.wikipedia.org/*"],
        "js": ["content_scripts.js"]
    }],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {},
    "permissions": ["tabs"]
}